{
 "compilerOptions": {
  "allowJs": true,
  "strict": false,
  "noImplicitAny": false,
  "target": "ES2022",
  "module": "ESNext",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "isolatedModules": true,
  "jsx": "preserve",
  "lib": [
   "dom",
   "ES2022"
  ],
  "moduleResolution": "Bundler",
  "noEmit": true,
  "outDir": "./build/",
  "resolveJsonModule": true,
  "preserveValueImports": false,
  "skipLibCheck": true,
  "strictFunctionTypes": false,
  "incremental": true,
  "baseUrl": ".",
 },
 "include": [
  "./**/*.tsx",
  "./**/*.ts"
 ],
 "exclude": [
  "node_modules"
 ]
}